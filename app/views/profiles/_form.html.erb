<%= form_with(model: profile, local: true) do |form| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :profile_name %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.number_field :user_id, id: :profile_user_id %>
  </div>

  <div class="field">
    <%= form.label :avatar_file_name %>
    <%= form.text_field :avatar_file_name, id: :profile_avatar_file_name %>
  </div>

  <div class="field">
    <%= form.label :avatar_content_type %>
    <%= form.text_field :avatar_content_type, id: :profile_avatar_content_type %>
  </div>

  <div class="field">
    <%= form.label :avatar_file_size %>
    <%= form.number_field :avatar_file_size, id: :profile_avatar_file_size %>
  </div>

  <div class="field">
    <%= form.label :avatar_updated_at %>
    <%= form.datetime_select :avatar_updated_at, id: :profile_avatar_updated_at %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
